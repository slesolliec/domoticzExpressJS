extends layout


block content
  h1= title
  p Welcome to #{title}

  each room in rooms
    .card(id= 'roomcard-' + room.name, class= 'temp-' + Math.floor(room.temp).toString())
      .heater= room.hitsOn > 0 ? 'On' : 'Off'
      .wanted= room.wanted + 'Â°'
      .temp!= Math.floor(room.temp).toString() + '.<small>' + (room.temp % 1).toFixed(1).substring(2) + '</small>'
      h2= room.name
